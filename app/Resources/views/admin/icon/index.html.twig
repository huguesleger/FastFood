{% extends ':admin:templateAdmin.html.twig' %}

  {% block content %}
    <h1>Mes icons</h1>
 <div class="x_panel">
                <div class="x_title">
                  <h2>Icons list <small>Retrouver ici vos icons disponible</small></h2>
                 
                  <div class="clearfix"></div>
                </div>
                <div class="x_content">

                  <section id="web-application">
 <div class="row fontawesome-icon-list">
        {% for icon in icons %}


    <div class="col-md-3 col-sm-4 col-xs-12">
        <img class="icon_food" src="{{asset('image/food/'~icon.nom~'.png')}}">{{icon.nom}}
    </div>

        {% endfor %}
     </div>
                  </section>
                </div>
 </div>
{% endblock %}
